@model  ASP_NET_MVC_Q2.Models.All

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/table.css" rel="stylesheet" />
</head>
<body>
    <div>
        <table class="fancytable">       
                <tr class="headerrow">
                    <td  align="center" valign="center">Product Name</td>
                    <td  align="center" valign="center">Locale</td>
                    <td  align="center" valign="center">Price</td>
                    <td  align="center" valign="center">Promote Price</td>
                    <td  align="center" valign="center">Create Date</td>
                </tr>

                @if (Model != null)
                {

                    <tr class="datarowodd">
                        <td align="center" valign="center">@Html.DisplayFor(model => model.Product_Name)</td>
                        <td align="center" valign="center"> @Html.DisplayFor(model => model.Locale)</td>
                        <td align="center" valign="center"> @Html.DisplayFor(model => model.Price)</td>
                        <td align="center" valign="center"> @Html.DisplayFor(model => model.Promote_Price)</td>
                        <td align="center" valign="center"> @Html.DisplayFor(model => model.Create_Date)</td>
                    </tr>

                }
        </table>
    </div>
</body>
</html>
